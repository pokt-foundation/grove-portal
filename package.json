{
  "name": "pocket-explorer",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s generate:types remix:build",
    "dev": "run-s generate:types remix:dev",
    "format": "prettier --write .",
    "generate:types": "run-p generate:types:*",
    "generate:local:cms": "graphql-codegen -r dotenv/config --config ./app/models/cms/codegen.config.yml",
    "generate:types:indexer": "graphql-codegen -r dotenv/config --config ./app/models/indexer/codegen.config.yml",
    "generate:types:poktscan": "graphql-codegen -r dotenv/config --config ./app/models/poktscan/codegen.config.yml",
    "generate:types:portal": "graphql-codegen -r dotenv/config --config ./app/models/portal/codegen.config.yml",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "remix:build": "remix build",
    "remix:dev": "remix dev",
    "test": "vitest",
    "test:unit:run": "vitest run --coverage",
    "test:unit:changed": "vitest run --coverage --changed",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3001 \"cypress open\"",
    "test:e2e:run": "start-server-and-test dev http://localhost:3001 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^0.3.1",
    "@directus/sdk": "^10.1.4",
    "@mantine/core": "^4.2.2",
    "@mantine/hooks": "^4.2.2",
    "@mantine/ssr": "^4.2.2",
    "@pokt-foundation/pocket-blocks": "1.0.21",
    "@pokt-foundation/pocketjs-isomorphic-provider": "^1.0.0",
    "@pokt-foundation/ui": "^0.3.9",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/server-runtime": "^1.13.0",
    "@remix-run/vercel": "^1.4.3",
    "@sentry/browser": "^6.19.7",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@stitches/react": "^1.2.8",
    "@vercel/node": "^1.14.0",
    "clsx": "^1.1.1",
    "d3-format": "^3.1.0",
    "dayjs": "^1.11.4",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-html-parser": "^2.0.2",
    "react-remark": "^2.1.0",
    "remix-auth": "^3.2.2",
    "remix-auth-auth0": "^1.3.7",
    "stripe": "^9.9.0",
    "styled-components": "^5.3.5",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/introspection": "^2.2.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@pokt-foundation/pocketjs-types": "^1.0.1",
    "@pokt-foundation/portal-types": "^0.0.1",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@sentry/types": "^6.19.7",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest-axe": "^3.5.4",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-html-parser": "^2.0.2",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "cypress": "10.4.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "jest-axe": "^6.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5",
    "vite": "^2.9.8",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.10.4"
  },
  "peerDependenciesMeta": {
    "@babel/preset-en@^7.1.6": {
      "optional": true
    }
  },
  "engines": {
    "node": "16.x"
  }
}
