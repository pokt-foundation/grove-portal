{
  "name": "pocket-explorer",
  "private": true,
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s generate:types remix:build",
    "dev": "run-s generate:types remix:dev",
    "format": "prettier --write .",
    "generate:types": "run-p generate:types:*",
    "generate:types:cms": "graphql-codegen -r dotenv/config --config ./app/models/cms/codegen.config.yml",
    "generate:types:indexer": "graphql-codegen -r dotenv/config --config ./app/models/indexer/codegen.config.yml",
    "generate:types:poktscan": "graphql-codegen -r dotenv/config --config ./app/models/poktscan/codegen.config.yml",
    "generate:types:portal": "graphql-codegen -r dotenv/config --config ./app/models/portal/codegen.config.yml",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "remix:build": "remix build",
    "remix:dev": "remix dev",
    "test": "vitest",
    "test:unit:run": "vitest run --coverage",
    "test:unit:changed": "vitest run --coverage --changed",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3001 \"cypress open\"",
    "test:e2e:run": "start-server-and-test dev http://localhost:3001 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "prepare": "husky install"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^0.3.1",
    "@directus/sdk": "^10.1.4",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/remix": "^5.10.3",
    "@pokt-foundation/pocket-blocks": "^2.0.3",
    "@pokt-foundation/pocketjs-isomorphic-provider": "^1.0.0",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/vercel": "^1.12.0",
    "@vercel/node": "^2.6.2",
    "clsx": "^1.1.1",
    "d3-format": "^3.1.0",
    "dayjs": "^1.11.4",
    "form-data": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-request": "5.1.0",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^3.0.8",
    "jwt-decode": "^3.1.2",
    "mailgun.js": "^8.2.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "remix-auth": "^3.2.2",
    "remix-auth-auth0": "^1.3.7",
    "stripe": "^9.9.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.13",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@pokt-foundation/pocketjs-types": "^1.0.1",
    "@pokt-foundation/portal-types": "^0.0.1",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@sentry/types": "^7.37.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest-axe": "^3.5.4",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "c8": "^7.11.2",
    "cross-env": "^7.0.3",
    "cypress": "10.4.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "jest-axe": "^6.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "engines": {
    "node": "16.x"
  }
}
